{
  "class": {
    "prefix": "class",
    "body": "class $1 ${2:extends ${3:Parent}} \n{\n\t$0\n}",
    "description": "class"
  },
  "constr": {
    "prefix": "constr",
    "body": "constructor($1) \n{\n\t${2:base.constructor($3)}\n\t$0\n}",
    "description": "constructor"
  },
  "for": {
    "prefix": "for",
    "body": "for (local $1; $2; $3) \n{\n\t$0\n}",
    "description": "for"
  },
  "foreach": {
    "prefix": "foreach",
    "body": "foreach ($1 in $2) \n{\n\t$0\n}",
    "description": "foreach"
  },
  "func": {
    "prefix": "func",
    "body": "function $1($2) \n{\n\t$0\n}",
    "description": "function"
  },
  "if": {
    "prefix": "if",
    "body": "if (${1:true}) \n{    ${0:$TM_SELECTED_TEXT}\n}",
    "description": "if"
  },
  "l": {
    "prefix": "l",
    "body": "local $0;",
    "description": "local"
  },
  "print": {
    "prefix": "print",
    "body": "print(\"$0\");",
    "description": "print"
  },
  "dprint": {
    "prefix": "dprint",
    "body": "if(bDebugPrinting)print(\"$0\");",
    "description": "bDebugPrinting"
  }
}