{
  "name": "SquirrelLanguageExtension",
  "displayName": "Etal TouchPoint",
  "description": "Etal (Squirrel) Language Keyword Colour and Snippets for Visual Studio Code",
  "version": "1.0.0",
  "publisher": "antonymorrish",
  "engines": {
    "vscode": "^1.12.0"
  },
  "license": "MIT",
  "keywords": [
    "etal",
    "Etal",
    "moc"
  ],
  "categories": [
    "Languages",
    "Snippets"
  ],
  "icon": "icon.png",
  "contributes": {
    "languages": [
      {
        "id": "Etal",
        "aliases": [
          "Etal",
          "etal",
          "moc"
        ],
        "extensions": [
          ".moc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "Etal",
        "scopeName": "source.Etal",
        "path": "./syntaxes/moc.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "Etal",
        "path": "./snippets/Etal-Squirrelcompletions.json"
      },
      {
        "language": "Etal",
        "path": "./snippets/Etal-dofiles.json"
      },
      {
        "language": "Etal",
        "path": "./snippets/Etal-snippets.json"
      },
      {
        "language": "Etal",
        "path": "./snippets/Etal-ICREvents.json"
      },
      {
        "language": "Etal",
        "path": "./snippets/Etal-ICRDialog.json"
      },
      {
        "language": "Etal",
        "path": "./snippets/Etal-ICRUtility.json"
      }
    ]
  },
  "files.associations": {
    "*.moc": "Etal"
  }
}